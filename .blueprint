created:
    - database/factories/ConferenceFactory.php
    - database/factories/VenueFactory.php
    - database/factories/SpeakerFactory.php
    - database/factories/TalkFactory.php
    - database/migrations/2024_08_28_215908_create_conferences_table.php
    - database/migrations/2024_08_28_215909_create_venues_table.php
    - database/migrations/2024_08_28_215910_create_speakers_table.php
    - database/migrations/2024_08_28_215911_create_talks_table.php
